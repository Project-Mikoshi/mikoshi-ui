{
  "compilerOptions": {
    "target": "es2015",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "outDir": "./dist/",
    "baseUrl": ".",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "declaration": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "paths": {
    "components/*": ["src/components/*"],
    "utils/*": ["src/utils/*"],
    "constants/*": ["src/constants/*"],
    "types/*": ["src/types/*"],
    "pages/*": ["Src/pages/*"],
    "svgs/*": ["src/svgs/*"]
    },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
