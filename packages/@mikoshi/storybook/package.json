{
  "name": "@mikoshi/storybook",
  "version": "0.1.4",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/anthony-y-zhu14/MikoshiUI.git",
    "directory": "packages/@mikoshi/storybook"
  },
  "description": "Storybook Playground for Mikoshi",
  "main": "index.js",
  "scripts": {
    "start": "start-storybook --quiet --disable-telemetry -p 6006",
    "build": "build-storybook --disable-telemetry --output-dir dist/",
    "chromatic": "npx chromatic --storybook-build-dir=dist",
    "clean": "rm -rf dist",
    "lint": "npm-run-all --parallel lint:eslint lint:stylelint",
    "lint:eslint": "eslint . --ignore-pattern node_modules/ --ext .jsx,.js,.ts,.tsx",
    "lint:stylelint": "echo 'no scss files'",
    "lint:fix": "eslint . --fix --ignore-pattern node_modules/ --ext .jsx,.js,.ts,.tsx"
  },
  "devDependencies": {
    "@mikoshi/application-components": "^0.2.4",
    "@mikoshi/core-components": "^0.2.4",
    "@mikoshi/styles": "^0.2.3",
    "@scaffold/eslint-config": "^0.1.3",
    "@scaffold/stylelint-config": "^0.1.3",
    "@scaffold/typescript-config": "^0.1.3",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitejs/plugin-react": "^2.0.0",
    "chromatic": "^6.7.4",
    "eslint": "^8.21.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-storybook": "^0.6.4",
    "html-webpack-plugin": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.54.3",
    "storybook-addon-themes": "^6.1.0",
    "stylelint": "^14.10.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "typescript": "^4.7.4",
    "vite": "3.0.9"
  },
  "nohoist": [
    "**/@storybook/**"
  ]
}
