{
  "name": "@mikoshi/storybook",
  "version": "0.10.0",
  "private": true,
  "description": "Storybook Playground for Mikoshi",
  "repository": {
    "type": "git",
    "url": "https://github.com/anthony-y-zhu14/MikoshiUI.git",
    "directory": "apps/@storybook"
  },
  "main": "index.js",
  "scripts": {
    "build": "storybook build --disable-telemetry --output-dir dist/",
    "clean": "rm -rf dist",
    "deploy": "npx chromatic --storybook-build-dir=dist",
    "lint": "npm-run-all --parallel lint:*",
    "lint:eslint": "eslint . --ext .jsx,.js,.ts,.tsx",
    "lint:stylelint": "echo 'no scss files'",
    "start": "storybook dev --quiet --disable-telemetry -p 6006"
  },
  "devDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mikoshi/components": "workspace:*",
    "@mikoshi/styles": "workspace:*",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@scaffold/eslint-config": "workspace:*",
    "@scaffold/stylelint-config": "workspace:*",
    "@scaffold/typescript-config": "workspace:*",
    "@storybook/addon-docs": "^9.1.2",
    "@storybook/builder-vite": "^9.1.2",
    "@storybook/react-vite": "^9.1.2",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "chromatic": "^13.1.3",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.90.0",
    "storybook": "^9.1.2",
    "stylelint": "^16.23.1",
    "stylelint-config-recommended": "^17.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "vite": "^7.1.3"
  },
  "nohoist": [
    "**/@storybook/**"
  ]
}
