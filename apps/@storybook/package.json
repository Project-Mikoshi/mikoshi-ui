{
  "name": "@mikoshi/storybook",
  "version": "0.5.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/anthony-y-zhu14/MikoshiUI.git",
    "directory": "apps/@storybook"
  },
  "description": "Storybook Playground for Mikoshi",
  "main": "index.js",
  "scripts": {
    "start": "start-storybook --quiet --disable-telemetry -p 6006",
    "build": "build-storybook --disable-telemetry --output-dir dist/",
    "chromatic": "npx chromatic --storybook-build-dir=dist",
    "clean": "rm -rf dist",
    "lint": "npm-run-all --parallel lint:eslint lint:stylelint",
    "lint:eslint": "eslint . --ignore-pattern node_modules/ --ext .jsx,.js,.ts,.tsx",
    "lint:stylelint": "echo 'no scss files'",
    "lint:fix": "eslint . --fix --ignore-pattern node_modules/ --ext .jsx,.js,.ts,.tsx"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mikoshi/application-components": "^0.6.0",
    "@mikoshi/core-components": "^0.8.0",
    "@mikoshi/styles": "^0.6.0",
    "@mui/icons-material": "^5.10.3",
    "@scaffold/eslint-config": "^0.2.0",
    "@scaffold/stylelint-config": "^0.1.3",
    "@scaffold/typescript-config": "^0.3.0",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "chromatic": "^6.7.4",
    "eslint": "^8.23.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.6",
    "eslint-plugin-storybook": "^0.6.4",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.54.9",
    "storybook-addon-themes": "^6.1.0",
    "stylelint": "^14.10.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0"
  },
  "nohoist": [
    "**/@storybook/**"
  ]
}
