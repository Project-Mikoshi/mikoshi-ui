{
  "name": "@mikoshi/storybook",
  "version": "0.9.1",
  "private": true,
  "description": "Storybook Playground for Mikoshi",
  "repository": {
    "type": "git",
    "url": "https://github.com/anthony-y-zhu14/MikoshiUI.git",
    "directory": "apps/@storybook"
  },
  "main": "index.js",
  "scripts": {
    "start": "storybook dev --quiet --disable-telemetry -p 6006",
    "build": "storybook build --disable-telemetry --output-dir dist/",
    "chromatic": "npx chromatic --storybook-build-dir=dist",
    "clean": "rm -rf dist",
    "lint": "npm-run-all --parallel lint:eslint lint:stylelint",
    "lint:eslint": "eslint . --ignore-pattern node_modules/ --ext .jsx,.js,.ts,.tsx",
    "lint:stylelint": "echo 'no scss files'",
    "lint:fix": "eslint . --fix --ignore-pattern node_modules/ --ext .jsx,.js,.ts,.tsx"
  },
  "devDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mikoshi/components": "^0.3.1",
    "@mikoshi/styles": "^0.10.0",
    "@mui/icons-material": "^5.15.1",
    "@mui/material": "^5.15.1",
    "@scaffold/eslint-config": "^0.4.0",
    "@scaffold/stylelint-config": "^0.3.0",
    "@scaffold/typescript-config": "^0.6.0",
    "@storybook/addon-actions": "^7.6.6",
    "@storybook/addon-essentials": "^7.6.6",
    "@storybook/addon-links": "^7.6.6",
    "@storybook/builder-vite": "^7.6.6",
    "@storybook/react": "^7.6.6",
    "@storybook/react-vite": "^7.6.6",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "chromatic": "^10.2.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.5",
    "storybook": "^7.6.6",
    "storybook-addon-themes": "^6.1.0",
    "stylelint": "^16.0.2",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-standard-scss": "^12.0.0",
    "vite": "^5.0.10"
  },
  "nohoist": [
    "**/@storybook/**"
  ]
}
