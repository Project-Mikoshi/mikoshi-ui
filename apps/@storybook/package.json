{
  "name": "@mikoshi/storybook",
  "version": "0.8.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/anthony-y-zhu14/MikoshiUI.git",
    "directory": "apps/@storybook"
  },
  "description": "Storybook Playground for Mikoshi",
  "main": "index.js",
  "scripts": {
    "start": "storybook dev --quiet --disable-telemetry -p 6006",
    "build": "storybook build --disable-telemetry --output-dir dist/",
    "chromatic": "npx chromatic --storybook-build-dir=dist",
    "clean": "rm -rf dist",
    "lint": "npm-run-all --parallel lint:eslint lint:stylelint",
    "lint:eslint": "eslint . --ignore-pattern node_modules/ --ext .jsx,.js,.ts,.tsx",
    "lint:stylelint": "echo 'no scss files'",
    "lint:fix": "eslint . --fix --ignore-pattern node_modules/ --ext .jsx,.js,.ts,.tsx"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mikoshi/components": "^0.2.1",
    "@mikoshi/styles": "^0.9.0",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.10",
    "@scaffold/eslint-config": "^0.3.0",
    "@scaffold/stylelint-config": "^0.2.0",
    "@scaffold/typescript-config": "^0.5.0",
    "@storybook/addon-actions": "^7.0.8",
    "@storybook/addon-essentials": "^7.0.8",
    "@storybook/addon-links": "^7.0.8",
    "@storybook/react": "^7.0.8",
    "@storybook/react-vite": "^7.0.8",
    "storybook": "^7.0.8",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "chromatic": "^6.17.4",
    "eslint": "^8.39.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.12",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.1",
    "storybook-addon-themes": "^6.1.0",
    "stylelint": "^14.16.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "typescript": "^5.0.4"
  },
  "nohoist": [
    "**/@storybook/**"
  ]
}
