{
  "name": "@mikoshi/storybook",
  "version": "0.6.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/anthony-y-zhu14/MikoshiUI.git",
    "directory": "apps/@storybook"
  },
  "description": "Storybook Playground for Mikoshi",
  "main": "index.js",
  "scripts": {
    "start": "start-storybook --quiet --disable-telemetry -p 6006",
    "build": "build-storybook --disable-telemetry --output-dir dist/",
    "chromatic": "npx chromatic --storybook-build-dir=dist",
    "clean": "rm -rf dist",
    "lint": "npm-run-all --parallel lint:eslint lint:stylelint",
    "lint:eslint": "eslint . --ignore-pattern node_modules/ --ext .jsx,.js,.ts,.tsx",
    "lint:stylelint": "echo 'no scss files'",
    "lint:fix": "eslint . --fix --ignore-pattern node_modules/ --ext .jsx,.js,.ts,.tsx"
  },
  "devDependencies": {
    "@emotion/styled": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@mikoshi/components": "^0.0.1",
    "@mikoshi/styles": "^0.7.0",
    "@mui/icons-material": "^5.10.16",
    "@mui/material": "^5.10.17",
    "@scaffold/eslint-config": "^0.2.0",
    "@scaffold/stylelint-config": "^0.1.3",
    "@scaffold/typescript-config": "^0.4.0",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/react": "^6.5.14",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "chromatic": "^6.11.4",
    "eslint": "^8.29.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-storybook": "^0.6.8",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.56.1",
    "storybook-addon-themes": "^6.1.0",
    "stylelint": "^14.16.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0"
  },
  "nohoist": [
    "**/@storybook/**"
  ]
}
